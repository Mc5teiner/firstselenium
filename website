    var user = {"GroupName":"CSN Alle","SessionId":null,"LoginResultCode":0,"IsAdmin":false,"InvoiceTransferIsVisible":false,"OverviewValutaIsVisible":false,"IsDetailedErrorMessageEnabled":true,"IsEnableDDesktopNotification":true,"DiscountsEditable":false,"Id":"2e079ffb-efe9-4687-b6e5-aa90123f24ea","UserName":"Mike.Steiner","NormalizedUserName":null,"Email":null,"NormalizedEmail":null,"EmailConfirmed":false,"PasswordHash":null,"SecurityStamp":"5620101e-ad44-4e1c-8083-fe50b40a938e","ConcurrencyStamp":"8733083b-82c8-4788-af21-a533b64e17eb","PhoneNumber":null,"PhoneNumberConfirmed":false,"TwoFactorEnabled":false,"LockoutEnd":null,"LockoutEnabled":false,"AccessFailedCount":0};

    $(document).ready(function () {
        $("#helpMenuLink").click(toolBarController.helpClickHandler);
        $('#searchLink').click(Search.openSearch);
        $("#batchFileListLink").click(toolBarController.batchFileListClickHandler);
        $('#sessionTimerContent').click(Tools.resetSessionTimerHandler);

        WebNotifications.authorize(user.IsEnableDDesktopNotification);

        //clicked-- > select first element
        $('#userMenuLink').keydown(function (e) {
            switch (e.which) {
                case 32: //32 -> space
                    $(this).click();
                    break;
                case 40: // down -> 40
                    e.preventDefault();
                    $(".dropdown-menu.show").find("a").first().focus();
                    break;

            }
        });

        $('.dropdown-menu a').keydown(function (e) {
            switch (e.which) {
                case 40: // down -> 40
                    e.preventDefault();
                    var nexta = $(this).parent().next().find('a');
                    nexta.focus();
                    break;
                case 38: // up -> 38
                    e.preventDefault();
                    var preva = $(this).parent().prev().find('a');
                    preva.focus();
                    break;
                //32 -> space
            }
        });
    });

</script>

    

