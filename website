<script id="fileUploadDialogTemplate" type="text/x-kendo-template">
    <a href="#" class='k-button k-disabled' role="button" data-activeif="importable" id='fileUploadbutton' onclick='performClick(theFile)'><i class='icon-upload'></i>Importieren</a>
    <form id="fileUploadForm" enctype="multipart/form-data" name="myForm">
        <input type='file' id='theFile' value="upload" hidden onchange="submitUpload(this)" accept=".xls,.xlsx,.xlsm,.csv" />
    </form>
</script>

<script>

    function importable() {
        var isImportable = false;

        var type = window.location.pathname.split('/')[1];

        switch (type) {
            case "Contract": 
                isImportable = $("#CanEdit").val().toLowerCase() === "true";
                break;

            case "Offer":
                isImportable =  $("#angId").val() !== "" && $("#CanEdit").val().toLowerCase() === "true";
                break;

            case "ContractChange": 
                var externalProcess = $("#vorgangId").val() !== "" && $("#vorgangId").val() !== "0";
                var isPriceChange = $("#ContractChangeActionType").val() == CONTRACT_CHANGE_ACTION_TYPE.PRICE_CHANGE;

                isImportable = 
                    $("#angId").val() !== "" && 
                    $("#CanEdit").val().toLowerCase() === "true" &&
                    !externalProcess &&
                    !isPriceChange;
                break;

            default:
                break;
        }

        return isImportable;
    }

    function performClick(elem) {
        if (elem && !$("#fileUploadbutton").hasClass("disabled") && document.createEvent) {
            var evt = document.createEvent("MouseEvents");
            evt.initEvent("click", true, false);
            elem.dispatchEvent(evt);
        }
    }

    function submitUpload(obj) {
        var form = $('#fileUploadForm')[0]; // You need to use standard javascript object here
        var modelId = $(form).parent().attr("data-id");
        var controller = window.location.pathname.split("/")[1];
        var formData = new FormData(form);

        formData.append('file', $(obj)[0].files[0]); 
        formData.append('id', modelId);

        KendoHelper.startProgress($("#fileUploadbutton"));
        AjaxHelper.ajaxSecureJson({
            url: '/' + controller + '/ImportData',
            data: formData,
            type: 'POST',
            contentType: false, // NEEDED, DON'T OMIT THIS (requires jQuery 1.6+)
            processData: false, // NEEDED, DON'T OMIT THIS
            success: function (data, textStatus, jqXHR) {
                Notifications.success(SharedResource.batchImportStart);
                KendoHelper.endProgress($("#fileUploadbutton"));
                $('#theFile').val("");
            },
            error: function (jqXHR, textStatus, errorThrown) {
                //Notification is shown by AjaxHelper.globalErrorHandler
                //Notifications.error(SharedResource.batchError);
                KendoHelper.endProgress($("#fileUploadbutton"));
                $('#theFile').val("");
            }
        });

        event.preventDefault();
    }
</script>


<script>
    $(function () {
        var isEditable = 'True'.toLowerCase() == 'true';
        
        if ('False' != 'True') {
            if ('False' == 'True') {
                $('#linkToEvplProcess').show();
                if (isEditable) {
                    KendoHelper.handleEditable(false, Windows.getTopWindow(), false, 'warning', SharedResource.editingWarningContractChange);
                    ContractChangeDetails.processWarningWasShown = true;
                } else {
                    KendoHelper.handleEditable(false);
                }
            } else {
                $('#linkToEvplProcess').hide();
                KendoHelper.handleEditable(isEditable);
            }
        } else {
            $("#kalk_datum").trigger("change");
        }

        if ("" != "") {
            Notifications.notify("", "notice");
        }
        if ($("#ContractBlocked").val() === "True") {
            Notifications.notify(ContractChangeResource.contractBlocked, "notice");
        }
    });
</script>


